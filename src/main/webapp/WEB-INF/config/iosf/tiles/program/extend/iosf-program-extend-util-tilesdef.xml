<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
      "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" 
      "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions> 
    
	<definition
		name="/back/find/*/*"
		extends="layout.back.popup"
	>
		<put-attribute
			name="content"
			value="/WEB-INF/jsp/iosf/com/program/back/extend/find/find_{1}_{2}.jsp" />
	</definition>
    
	<definition
		name="/front/find/*/*"
		extends="layout.front.popup"
	>
		<put-attribute
			name="content"
			value="/WEB-INF/jsp/iosf/com/program/front/extend/find/find_{1}_{2}.jsp" />
	</definition>
	
</tiles-definitions>
