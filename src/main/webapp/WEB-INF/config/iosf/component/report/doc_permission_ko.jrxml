	<parameter name="EVENT_SUBMIT_SEQ" class="java.lang.Long"/>
	<parameter name="REPORT_SRC" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT DATE_FORMAT(NOW(), '%Y 년 %m 월 %d 일') NOW_DT 
	, A.USER_NM
	, A.DEPT_NM
	, B.`YEAR`
	, FN_GET_CODE('school_term', B.TERM) TERM
	, CONCAT(DATE_FORMAT(STR_TO_DATE(D.START_DT, '%Y%m%d'), '%Y-%m-%d'), ' (', SUBSTR( _UTF8'일월화수목금토', DAYOFWEEK(STR_TO_DATE(D.START_DT, '%Y%m%d')), 1), ')') START_DT
	, CONCAT(DATE_FORMAT(STR_TO_DATE(D.END_DT, '%Y%m%d'), '%Y-%m-%d'), ' (', SUBSTR( _UTF8'일월화수목금토', DAYOFWEEK(STR_TO_DATE(D.START_DT, '%Y%m%d')), 1), ')') END_DT
	, CONCAT(DATE_FORMAT(STR_TO_DATE(B.PAY_START_DT, '%Y%m%d%H%i%s'), '%Y-%m-%d'), ' (', SUBSTR( _UTF8'일월화수목금토', DAYOFWEEK(STR_TO_DATE(B.PAY_START_DT, '%Y%m%d%H%i%s')), 1), ') ', DATE_FORMAT(STR_TO_DATE(B.PAY_START_DT, '%Y%m%d%H%i%s'), '%H:%i')) PAY_START_DT
	, CONCAT(DATE_FORMAT(STR_TO_DATE(B.PAY_END_DT, '%Y%m%d%H%i%s'), '%Y-%m-%d'), ' (', SUBSTR( _UTF8'일월화수목금토', DAYOFWEEK(STR_TO_DATE(B.PAY_END_DT, '%Y%m%d%H%i%s')), 1), ') ', DATE_FORMAT(STR_TO_DATE(B.PAY_END_DT, '%Y%m%d%H%i%s'), '%H:%i')) PAY_END_DT
	, (SELECT FORMAT(PAY_PRICE, 0) FROM IOSF_EVENT_SUBMIT_INVOICE AA WHERE AA.EVENT_SUBMIT_SEQ = A.EVENT_SUBMIT_SEQ ORDER BY AA.EVENT_SUBMIT_INVOICE_SEQ LIMIT 1) PAY_PRICE
	, CONCAT(DATE_FORMAT(DATE_ADD(STR_TO_DATE(B.PAY_END_DT, '%Y%m%d%H%i%s'), INTERVAL 1 DAY), '%Y-%m-%d'), ' (', SUBSTR( _UTF8'일월화수목금토', DAYOFWEEK(DATE_ADD(STR_TO_DATE(B.PAY_END_DT, '%Y%m%d%H%i%s'), INTERVAL 1 DAY)), 1), ')') PAY_NEXT_DT
	, F.VACCOUNT_NO 
FROM IOSF_EVENT_SUBMIT A
INNER JOIN IOSF_EVENT B ON A.EVENT_SEQ = B.EVENT_SEQ
INNER JOIN IOSF_EVENT_SUBMIT_ROOM_MAPP D ON A.EVENT_SUBMIT_SEQ = D.EVENT_SUBMIT_SEQ
INNER JOIN IOSF_ROOM E ON D.ROOM_SEQ = E.ROOM_SEQ
INNER JOIN IOSF_VACCOUNT F ON A.EVENT_SUBMIT_SEQ = F.EVENT_SUBMIT_SEQ
WHERE A.EVENT_SUBMIT_SEQ = $P{EVENT_SUBMIT_SEQ}]]>
	</queryString>
