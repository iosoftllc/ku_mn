	<queryString>
		<![CDATA[SELECT A.USER_NM
	, FN_GET_CODE('nation_cd', A.NATION_CD) NATION_NM
	, CONCAT(FN_GET_CODE('bd_cd', D.BD_CD), ' ', C.ROOM_NM) ADDR
	, DATE_FORMAT(STR_TO_DATE(B.START_DT, '%Y%m%d'), '%Y년 (Year)  %m 월 (Month)  %d 일 (Date)') DT
	, DATE_FORMAT(NOW(), '%Y년 (Year)  %m 월 (Month)  %d 일 (Date)') NOW_DT
	, $P{STAFF_NM} AS STAFF_NM
FROM IOSF_EVENT_SUBMIT A
INNER JOIN IOSF_EVENT_SUBMIT_ROOM_MAPP B ON A.EVENT_SUBMIT_SEQ = B.EVENT_SUBMIT_SEQ
INNER JOIN IOSF_ROOM C ON B.ROOM_SEQ = C.ROOM_SEQ
INNER JOIN IOSF_ROOM_TYPE D ON C.ROOM_TYPE_SEQ = D.ROOM_TYPE_SEQ
WHERE A.EVENT_SUBMIT_SEQ =  $P{EVENT_SUBMIT_SEQ}
ORDER BY B.START_DT ASC
LIMIT 1]]>
	</queryString>
